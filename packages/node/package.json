{
  "name": "@given3/node",
  "version": "2.1.0",
  "description": "Node adapter for Given3",
  "keywords": [
    "given",
    "node",
    "test",
    "testing",
    "typescript"
  ],
  "license": "MIT",
  "private": false,
  "author": "Chris Pardy <chris.pardy@gmail.com>",
  "homepage": "https://github.com/chris-pardy/given3",
  "repository": {
    "type": "git",
    "url": "https://github.com/chris-pardy/given3.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "typecheck": "tsc",
    "build": "tsup src/index.mts --format esm --dts --tsconfig tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "typescript": "catalog:",
    "tsup": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "eslint-config-prettier": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "dependencies": {
    "@given3/core": "workspace:@given3/core@*"
  }
}
