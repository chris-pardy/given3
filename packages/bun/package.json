{
  "name": "@given3/bun",
  "version": "2.1.0",
  "description": "Bun adapter for Given3",
  "license": "MIT",
  "private": false,
  "author": "Chris Pardy <chris.pardy@gmail.com>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist", "LICENSE.md", "README.md"],
  "type": "module",
  "scripts": {
    "test": "bun test",
    "clean": "rm -rf dist",
    "typecheck": "tsc",
    "build": "tsup src/index.mts --format esm --dts --tsconfig tsconfig.build.json --external bun:test",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "bun-types": "^1.2.0",
    "typescript": "catalog:",
    "tsup": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "eslint-config-prettier": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "dependencies": {
    "@given3/core": "workspace:@given3/core@*"
  }
}
