{
  "name": "@given3/vitest",
  "version": "2.1.0",
  "description": "Vitest adapter for Given3",
  "keywords": [
    "given",
    "vitest",
    "test",
    "testing",
    "typescript"
  ],
  "license": "MIT",
  "private": false,
  "author": "Chris Pardy <chris.pardy@gmail.com>",
  "homepage": "https://github.com/chris-pardy/given3",
  "repository": {
    "type": "git",
    "url": "https://github.com/chris-pardy/given3.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "typecheck": "tsc",
    "build": "tsup src/index.mts --format esm --dts --tsconfig tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "test": "vitest --no-watch",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "tsup": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "eslint-config-prettier": "catalog:",
    "typescript-eslint": "catalog:",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@given3/core": "workspace:@given3/core@*"
  }
}
